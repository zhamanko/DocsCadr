{
  "name": "docscadr",
  "version": "0.0.1",
  "private": true,
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "homepage": "https://github.com/zhamanko/DocsCadr#readme",
  "bugs": {
    "url": "https://github.com/zhamanko/DocsCadr/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhamanko/DocsCadr.git"
  },
  "license": "ISC",
  "author": "zhaman",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\" ",
    "start": "electron .",
    "build": "vite build",
    "preview": "vite preview",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "electron-updater": "^6.6.2",
    "jszip": "^3.10.1",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "dotenv": "^17.2.1",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.docscadr.app",
    "productName": "DocsCadr",
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    },
    "files": [
      "dist",
      "main.js",
      "preload.js",
      "server/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "server",
        "to": "server"
      }
    ],
    "asar": false,
    "win": {
      "target": "dir"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "axios": "^1.10.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7"
  },
  "publish": [
    {
      "provider": "github",
      "owner": "zhamanko",
      "repo": "DocsCadr"
    }
  ]
}
